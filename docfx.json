{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "_csharplang/**",
          "_site/**",
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "csharp-7.0/binary-literals.md"
        ],
        "src": "_csharplang/proposals",
        "dest": "csharp/language-reference/proposals"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "_site/**",
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/renzeyutest/breadcrumb/toc.json",
      "extendBreadcrumb": true
    },
    "exportRawModel": true,
    "fileMetadata": {},
    "template": [],
    "dest": "renzeyutest",
    "markdownEngineName": "markdig"
  }
}
